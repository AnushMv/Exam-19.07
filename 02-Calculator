function Calculator(num1,num2,sighn){
    this.num1 = num1;
    this.num2 = num2;
    this.sighn = sighn;
    if(this.sighn === '+'){
        return this.num1 + this.num2;
    } else if(this.sighn === '-'){
        return this.num1 - this.num2;
    } else if(this.sighn === '*'){
        return this.num1 * this.num2;
    } else if(this.sighn === '/'){
        return this.num1 / this.num2;
    } 
}
